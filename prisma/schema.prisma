generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_CONNECTION_URI")
}

model Instance {
  id             String   @id @default(uuid())
  name           String
  phone          String
  status         String
  webhook        String?
  createdAt      DateTime @default(now())
  updatedAt      DateTime @updatedAt
}
